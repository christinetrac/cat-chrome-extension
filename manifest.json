{
    "name": "sad cats",
    "version": "0.0",
    "description": "replaces all images with sad cats",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent"],
    "icons": {
        "16": "images/cat_16.png",
        "32": "images/cat_32.png",
        "48": "images/cat_48.png",
        "128": "images/cat_128.jpg"
    },
    "page_action": {
        "default_icon": {
            "16": "images/cat_16.png",
            "32": "images/cat_32.png",
            "48": "images/cat_48.png",
            "128": "images/cat_128.jpg"
        }
    },
    "background": {
        "scripts": ["scripts.js"],
        "persistent": false
    }
}